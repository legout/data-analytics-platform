{
  "ServerProxy": {
    "servers": {
      "code-server": {
        "command": [
          "bash",
          "-lc",
          "python -m jupyter_vscode_proxy --port {port}"
        ],
        "timeout": 20,
        "launcher_entry": {
          "title": "VS Code",
          "icon_path": ""
        }
      }
    }
  }
}
