{
    "ServerProxy": {
        "servers": {
            "marimo": {
                "command": [
                    "marimo",
                    "edit",
                    "--host=127.0.0.1",
                    "--port={port}",
                    "--base-url={base_url}marimo",
                    "--headless"
                ],
                "timeout": 30,
                "absolute_url": true,
                "launcher_entry": {
                    "title": "Marimo (uv)",
                    "icon_path": ""
                }
            }
        }
    }
}